$TTL 604800
;
@		IN SOA		www.httpbootipv6.local.	root.httpbootipv6.local. (
				2017112800	; serial
				604800		; refresh
				86400		; retry
				2419200		; expiry
				604800 )	; minimum

httpbootipv6.local.	IN NS		www.httpbootipv6.local.
httpbootipv6.local.	IN NS		httpbootipv6.local.
;		IN data		file for local loopback interface
httpbootipv6.local.	IN AAAA		2001:db8:f00f:cafe::1
www		IN AAAA		2001:db8:f00f:cafe::1
www.httpbootipv6.local	IN AAAA		2001:db8:f00f:cafe::1
