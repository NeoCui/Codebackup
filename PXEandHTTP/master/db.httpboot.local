$TTL 1w
;
@		IN SOA		www.httpboot.local.	root.httpboot.local. (
				2017112800	; serial
				1w		; refresh
				1d		; retry
				4w		; expiry
				1w )		; minimum

httpboot.local.	IN NS		www.httpboot.local.
httpboot.local.	IN NS		httpboot.local.
;		IN data		file for local loopback interface
httpboot.local.	IN AAAA		::1
httpboot.local.	IN A		192.168.5.1
www.httpboot.local.	IN A		192.168.5.1
www.httpboot.local.	IN A		192.168.5.1
