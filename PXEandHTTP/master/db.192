;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	www.httpboot.local. root.httpboot.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	www.httpboot.local.
@	IN	AAAA	::1
www	IN	A	192.168.5.1
1	IN	PTR	www.httpboot.local.
